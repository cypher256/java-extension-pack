{
  "name": "java-extension-pack-jdk",
  "displayName": "Java Extension Pack JDK Bundle",
  "description": "Includes multiple versions LTS JDK for out-of-the-box development",
  "icon": "README_icon.png",
  "version": "1.0.0",
  "publisher": "Pleiades",
  "repository": {
    "url": "https://github.com/cypher256/pleiades.io/issues?q="
  },
  "author": {
    "name": "Shinji Kashihara"
  },
  "engines": {
    "vscode": "^1.54.0"
  },
  "keywords": [
    "java",
    "debugger",
    "junit",
    "gradle",
    "maven",
    "spring"
  ],
  "categories": [
    "Programming Languages",
    "Linters",
    "Debuggers",
    "Formatters",
    "Snippets",
    "Extension Packs"
  ],
  "extensionPack": [
    "vscjava.vscode-java-pack",
    "vscjava.vscode-gradle",
    "vmware.vscode-boot-dev-pack",
    "redhat.vscode-community-server-connector"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {},
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/decompress": "^4.2.4",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.54.0",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "decompress": "^4.2.1"
  }
}
