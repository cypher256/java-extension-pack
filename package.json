{
  "name": "java-extension-pack-jdk",
  "displayName": "Java Extension Pack JDK Bundle",
  "description": "JDK installation and JAVA_HOME settings are NOT required",
  "icon": "README_icon.png",
  "version": "17.2.11",
  "publisher": "Pleiades",
  "repository": {
    "url": "https://github.com/cypher256/java-extension-pack.git"
  },
  "author": {
    "name": "Shinji Kashihara"
  },
  "engines": {
    "vscode": "^1.54.0"
  },
  "keywords": [
    "jdk",
    "JAVA_HOME",
    "java",
    "gradle",
    "lombok",
    "spring"
  ],
  "categories": [
    "Programming Languages",
    "Linters",
    "Debuggers",
    "Formatters",
    "Snippets",
    "Extension Packs"
  ],
  "extensionPack": [
    "vscjava.vscode-java-pack",
    "vscjava.vscode-gradle",
    "vmware.vscode-boot-dev-pack",
    "redhat.vscode-community-server-connector"
  ],
  "extensionDependencies": [],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "l10n": "./l10n",
  "contributes": {},
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "_l10n": "npx @vscode/l10n-dev export --outDir ./l10n ./src"
  },
  "devDependencies": {
    "@types/decompress": "^4.2.4",
    "@types/glob": "^8.1.0",
    "@types/lodash": "^4.14.192",
    "@types/mocha": "^10.0.1",
    "@types/node": "18.x",
    "@types/vscode": "^1.54.0",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "@vscode/l10n-dev": "^0.0.24",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.38.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "axios": "^1.3.5",
    "compare-versions": "^6.0.0-rc.1",
    "decompress": "^4.2.1",
    "jdk-utils": "^0.4.4",
    "lodash": "^4.17.21"
  }
}
